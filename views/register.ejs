
<%- include('partials/header')-%>





<div class="pusher">
    <div class="ui grid">       

<div class="container" style="margin-top:0px">
    <div class="col-md-4 col-md-offset-4">
        <div class="panel panel-default">
            <div class="panel-heading"><h3 class="panel-title"><strong>Sign Up Form</strong></h3></div>
            <div class="panel-body">
                <form action="/register" method="POST" role="form" id="loginForm">
                    <div class="form-group">
                        <label for="exampleInputEmail1">Registration Number</label>
                        <input type="text" name="username" value="<%=studentData.username%>" style="border-radius:0px" class="form-control"  placeholder="Reg Number" readonly >
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Password </label>
                        <input type="password" name="password" placeholder="password" v-model="password" style="border-radius:0px" class="form-control" id="userPassword" placeholder="Password" onkeyup='check();'>
                    </div>
                      <button type="submit" class="btn btn-sm btn-default">Sign in</button>
                    </form>
                  </div>
                </div>


                    
                
            </div>
        </div>
    </div>
</div>


    </div>
</div>

<%- include('partials/footer')-%>



